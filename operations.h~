#pragma once

#include <math.h>

double exp_(double number) {
  return exp(number);
}

double pow_(double base, double power) {
  return pow(base, power);
}

double sin_(double number) {
  return sin(number);
}

double add(double num1, double num2) {
  return num1 + num2;
}

double sub(double num1, double num2) {
  return num1 - num2;
}

double mul(double num1, double num1) {
  return num1 * num2;
}

double div(double num1, double num2) {
  return num1 / num2;
}

double mod(double num1, double num2) {
  return num1 % num2;
}
